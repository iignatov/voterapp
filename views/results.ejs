<table class='table table-hover table-striped'>
  <% for (let [place_id, place_name] of Object.entries(CHOICES)) { %>
    <tr>
      <th><%= place_name %></th>
      <td><%= votes[place_id] || 0 %></td>
      <td><%= '#'.repeat(votes[place_id] || 0) %></td>
    </tr>
  <% } %>
</table>
