<table class='table table-hover table-striped'>
  <% for (let [place_id, place_name] of Object.keys(CHOICES).map(x => [x, CHOICES[x]])) { %>
    <tr>
      <th><%= place_name %></th>
      <td><%= votes[place_id] || 0 %></td>
      <td><%= '#'.repeat(votes[place_id] || 0) %></td>
    </tr>
  <% } %>
</table>
<p><a href='/'>Гласувайте още</a></p>
